  <DataSets>
    <DataSet Name="SalesByTerritory">
      <Query>
        <DataSourceName>AdventureWorks2017</DataSourceName>
        <CommandText>SELECT        YEAR(Sales.SalesOrderHeader.OrderDate) AS OrderYear, Sales.SalesTerritory.TerritoryID, Sales.SalesTerritory.Name AS Territory, SUM(Sales.SalesOrderHeader.TotalDue) AS Sales
FROM            Sales.SalesOrderHeader INNER JOIN
                         Sales.SalesTerritory ON Sales.SalesOrderHeader.TerritoryID = Sales.SalesTerritory.TerritoryID AND Sales.SalesOrderHeader.TerritoryID = Sales.SalesTerritory.TerritoryID AND 
                         Sales.SalesOrderHeader.TerritoryID = Sales.SalesTerritory.TerritoryID AND Sales.SalesOrderHeader.TerritoryID = Sales.SalesTerritory.TerritoryID
GROUP BY YEAR(Sales.SalesOrderHeader.OrderDate), Sales.SalesTerritory.TerritoryID, Sales.SalesTerritory.Name
ORDER BY OrderYear, Territory, Sales</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="OrderYear">
          <DataField>OrderYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TerritoryID">
          <DataField>TerritoryID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Territory">
          <DataField>Territory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Sales">
          <DataField>Sales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
