  <DataSets>
    <DataSet Name="SalesDetails">
      <Query>
        <DataSourceName>AdventureWorks2017</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@TerritoryIDList">
            <Value>=Parameters!TerritoryIDList.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@YearList">
            <Value>=Parameters!YearList.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Sales.SalesOrderHeader.OrderDate, Sales.SalesTerritory.TerritoryID, Sales.SalesTerritory.Name, Sales.SalesOrderHeader.CustomerID, Sales.SalesOrderHeader.SalesOrderID, 
                         Sales.SalesOrderHeader.TotalDue
FROM            Sales.SalesOrderHeader INNER JOIN
                         Sales.SalesTerritory ON Sales.SalesOrderHeader.TerritoryID = Sales.SalesTerritory.TerritoryID
WHERE        (Sales.SalesTerritory.TerritoryID = @TerritoryIDList) AND (YEAR(Sales.SalesOrderHeader.OrderDate) = @YearList)
Order by Sales.SalesOrderHeader.OrderDate ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="OrderDate">
          <DataField>OrderDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="TerritoryID">
          <DataField>TerritoryID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CustomerID">
          <DataField>CustomerID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SalesOrderID">
          <DataField>SalesOrderID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TotalDue">
          <DataField>TotalDue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
