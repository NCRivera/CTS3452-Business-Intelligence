          <DataField>Territory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="TerritoryList">
      <Query>
        <DataSourceName>AdventureWorks2017</DataSourceName>
        <CommandText>SELECT DISTINCT
	T.TerritoryID, 
	T.Name AS Territory
FROM
	Sales.SalesOrderHeader AS SOH INNER JOIN
                         Sales.SalesTerritory AS T ON SOH.TerritoryID = T.TerritoryID
ORDER BY T.Name ASC
;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="TerritoryID">
          <DataField>TerritoryID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Territory">
          <DataField>Territory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Years">
      <Query>
        <DataSourceName>AdventureWorks2017</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@TerritoryIDList">
            <Value>=Parameters!TerritoryIDList.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT 
	YEAR(OrderDate) AS Years
FROM 
	Sales.SalesOrderHeader
WHERE
	(TerritoryID IN (@TerritoryIDList))
ORDER BY Years DESC;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Years">
          <DataField>Years</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
